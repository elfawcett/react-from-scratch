{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "arrow-parens": false,
    "trailing-comma": false,
    "member-access": false,
    "interface-over-type-literal": false
  },
  "linterOptions": {
    "exclude": ["./node_modules/**/*", "./src/webpack.config.ts"]
  }
}
